{
  "name": "vizier",
  "version": "1.0.0-beta.4",
  "author": "Ryan Mark <ryan.mark@voxmedia.com>",
  "description": "Create, manage and publish ai2html projects",
  "license": "BSD-3-Clause",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:win": "node .electron-vue/build.js && electron-builder --win",
    "build:mac": "node .electron-vue/build.js && electron-builder --mac",
    "build:linux": "node .electron-vue/build.js && electron-builder --linux",
    "build:all": "node .electron-vue/build.js && electron-builder -mwl",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "build:publish": "node .electron-vue/build.js && electron-builder --mac -p always",
    "electronbuild": "electron-builder",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "productName": "Vizier",
    "appId": "com.voxmedia.vizier",
    "generateUpdatesFilesForAllChannels": true,
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "mac": {
      "icon": "build/icons/icon.icns",
      "target": "zip"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": "portable"
    },
    "linux": {
      "icon": "build/icons",
      "target": "appimage"
    },
    "publish": [
      {
        "provider": "s3",
        "bucket": "apps.voxmedia.com",
        "path": "vizapp"
      },
      {
        "provider": "github",
        "owner": "voxmedia",
        "repo": "viz-app"
      }
    ]
  },
  "dependencies": {
    "at-ui": "^1.3.3",
    "at-ui-style": "^1.5.1",
    "aws-sdk": "^2.229.1",
    "co": "^4.6.0",
    "co-fs-extra": "^1.2.1",
    "electron-json-storage": "^4.0.2",
    "electron-log": "^2.2.14",
    "electron-updater": "^2.21.4",
    "gulp": "4",
    "gulp-chmod": "^2.0.0",
    "gulp-data": "^1.3.1",
    "gulp-rename": "^1.2.2",
    "gulp-template": "^5.0.0",
    "js-yaml": "^3.11.0",
    "lodash": "^4.17.10",
    "luxon": "^1.0.0",
    "rimraf": "^2.6.2",
    "underscore.string": "^3.3.4",
    "uuid": "^3.2.1",
    "vue": "^2.3.3",
    "vue-electron": "^1.0.6",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "asar": "^0.14.3",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chai": "^4.0.0",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": ">=2.0.0-beta.7 <2.1",
    "electron-builder": "^19.19.1",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-publisher-s3": "^20.9.0",
    "eslint": "^4.4.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.0.2",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.3",
    "require-dir": "^0.3.0",
    "sass-loader": "^6.0.7",
    "spectron": "^3.7.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  }
}
